	body { 
		margin: 0;
		padding: 0;
		box-sizing: border-box;
		background-color: #dae1e3;
	}
	header { height: 10px;
			 color: white;
			 background-color: gray;
			 display: flex;
			 justify-content: flex-end;
	}
header a { margin-right: 20px;
		   padding-left: 10px;
		   margin-top: 30px;
		   color: #e9daed;
		   text-decoration: none;
		   font-family: Impact;
		   font-size: 30px;
}
header a:hover{color: #e6d85c;
}
.ab{
	text-decoration: none; 
	color: white;
	font-family: Bahnschrift SemiBold,Consolas,Franklin Gothic Medium;
	margin-left: 120px;
}
	
	.ControllerHub {
	display: flex;
	text-decoration: none;
	color: white;
	background-color: gray;
	border-bottom-right-radius: 4px;
	border-bottom-left-radius: 4px;
	font-family: Bahnschrift SemiBold,Consolas,Franklin Gothic Medium;
	}
	.ControllerHub a:hover {color: #cce8d7;text-decoration: none;}
	.Controller-parent {
		position: relative;
	}
	.Controller-Content, .Controller-Content-background, .Controller-Front-Images-Jumpi, .Controller-Front-Painter{
		width: 100%;
		height: 200px;
		position: absolute;
	}
	.Controller-Front-Images-Jumpi{
		display: flex;
		justify-content: right;
		margin-top: 70px;
		z-index: 1;
	}
	.Controller-Content {
		z-index: 3;
		height: 370px; 
		margin-top: -15px;
		color: #544569;
		display: flex;
		justify-content: center; 
		
		font-size: 20px;
		font-family: Consolas,Comic Sans MS;
		
		
	}
	.Controller-Front-Painter{
		z-index: 2;
		height: 362px;
		width: 350px;
		margin-top: 45px;
		margin-left: 200px;
		background-color: transparent;
		background-image: linear-gradient(to right, rgba(255, 255, 255, 0), #dae1e3, rgba(255, 255, 255, 0));
		display: flex;
		justify-content: center;
		
	}

	.Controller-Content-background {
			z-index: 0;
			margin-top: 45px;
			background-color: #dae1e3;
			display: flex;
			justify-content: center;
			
	}
.version{
	font-family: Bahnschrift SemiBold,Consolas,Franklin Gothic Medium;
	
}